{
  "name": "popcorn-time",
  "version": "1.0.0",
  "repository": "git@github.com:Stoope/popcorn-time.git",
  "author": "Stepan Kurennykh <stoope@outlook.com>",
  "main": "./src/index.js",
  "license": "MIT",
  "scripts": {
    "parcel:dev": "rm -rf build/* && parcel ./src/app/index.html -d build/",
    "electron:dev": "ELECTRON_START_URL=http://localhost:1234 electron .",
    "start": "concurrently \"yarn parcel:dev\" \"yarn electron:dev\" \"yarn test:watch\"",
    "precommit": "lint-staged",
    "build": "rm -rf build/* && parcel build ./src/app/index.html -d build/ --public-url ./ && electron-builder",
    "flow-typed": "flow-typed install",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "build": {
    "appId": "popcorn.time",
    "mac": {
      "category": "public.app-category.entertainment"
    },
    "files": [
      "./src/index.js",
      "build/*"
    ]
  },
  "dependencies": {
    "@material-ui/core": "1.4.2",
    "@material-ui/icons": "2.0.0",
    "babel-polyfill": "6.26.0",
    "deepmerge": "2.1.1",
    "lodash": "4.17.10",
    "memoize-one": "4.0.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-saga": "0.16.0",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "concurrently": "3.6.1",
    "electron": "2.0.5",
    "electron-builder": "20.26.0",
    "electron-devtools-installer": "2.2.4",
    "electron-packager": "12.1.0",
    "electron-rebuild": "1.8.1",
    "electron-reload": "1.2.5",
    "eslint": "5.2.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-react": "7.10.0",
    "flow-bin": "0.77.0",
    "flow-typed": "2.5.1",
    "jest": "23.4.2",
    "lint-staged": "7.2.0",
    "parcel-bundler": "1.9.7",
    "prettier": "1.14.0"
  },
  "optionalDependencies": {
    "fsevents": "1.2.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --max-warnings=0",
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "alias": {
    "~": "./src/app",
    "~reduxState": "./src/app/HOCs/ReduxWithMidleware/reducers/reduxState"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  }
}
